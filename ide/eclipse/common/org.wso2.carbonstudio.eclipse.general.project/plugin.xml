<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.ui.newWizards">
		<wizard name="Registry Resources Project" 
			category="org.wso2.carbonstudio.eclipse.capp.project/org.wso2.carbonstudio.eclipse.repository"
			class="org.wso2.carbonstudio.eclipse.general.project.ui.wizard.GeneralProjectWizard"
            finalPerspective="org.eclipse.jst.j2ee.J2EEPerspective"
			id="org.wso2.carbonstudio.eclipse.general.project"
			wizardManifest="project_wizard.xml"
			icon="icons/general-project-12.png"
			project="true">
			<description>Registry Resources Project</description>
		</wizard>
   </extension>
      <extension point="org.eclipse.core.resources.natures"
		id="org.wso2.carbonstudio.eclipse.general.project.nature" name="General Project Nature">
		<runtime>
			<run
				class="org.wso2.carbonstudio.eclipse.general.project.nature.GeneralProjectNature">
			</run>
		</runtime>
	</extension>
	<extension point="org.eclipse.ui.ide.projectNatureImages">
		<image id="org.wso2.carbonstudio.eclipse.esb.project.natureImage"
			natureId="org.wso2.carbonstudio.eclipse.general.project.nature" icon="icons/general-project-12.png">
		</image>
	</extension>
</plugin>
