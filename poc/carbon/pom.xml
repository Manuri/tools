    		<id>wso2-maven2-repo</id>
    		<name>WSO2 Maven2 Repo</name>
    		<url>http://dist.wso2.org/maven2/</url>
    	</repository>
    </repositories>
    <properties>
        <carbon.tools.version>1.1.0.SNAPSHOT</carbon.tools.version>
        <server.plugins.version>1.1.0.SNAPSHOT</server.plugins.version>      
        <logging.tools.version>1.1.0.SNAPSHOT</logging.tools.version>                	                  
